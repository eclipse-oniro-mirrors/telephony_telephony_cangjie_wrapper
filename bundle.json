{
    "name": "@ohos/telephony_cangjie_wrapper",
    "version": "6.1",
    "description": "The telephony_telephony_cangjie_wrapper provides call management capabilities for developers on OpenHarmony.",
    "publishAs": "code-segment",
    "homePage": "https://gitcode.com/openharmony-sig",
    "repository": "https://gitcode.com/openharmony-sig/telephony_telephony_cangjie_wrapper",
    "license": "Apache License 2.0",
    "private": false,
    "scripts": {},
    "envs": [],
    "dirs": [],
    "author": {},
